{
  "name": "webpack-babel6-on-express",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config ./webpack.prod.js",
    "dev:client": "webpack-dev-server --config ./webpack.dev.js",
    "dev:server": "set NODE_ENV=development && babel-watch ./src/server/server.js",
    "prod:server": "set NODE_ENV=production && node ./src/server/server.js"
  },
